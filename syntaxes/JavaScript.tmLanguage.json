{
  "scopeName": "source.js",
  "name": "JavaScript",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [{
        "name": "keyword.control.js",
        "match": "\\b(if|else|for|while|do|switch|case|break|continue|return|function|var|let|const)\\b"
      }]
    },
    "strings": {
      "name": "string.quoted.double.js",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.js",
          "match": "\\\\."
        }
      ]
    }
  }
} 